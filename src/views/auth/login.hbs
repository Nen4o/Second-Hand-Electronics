<section id="loginPage">
    <form id='loginForm' method="POST">
        <label for="email">Email:</label>
        <input type="text" id="email" name="email" value="{{loginData.email}}" placeholder="Email">

        <label for="password">Password:</label>
        <input type="password" id="password" value="{{loginData.password}}" name="password" placeholder="Password">

        <input type="submit" class="register" value="Login">
    </form>
</section>